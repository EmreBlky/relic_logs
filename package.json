{
  "name": "relic_logs",
  "version": "2.4.0",
  "description": "Request life cycle logging, generating atomic logs for each request",
  "main": "main.js",
  "dependencies": {
    "trace": "^3.0.0",
    "uuid": "latest"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.13.4",
    "mocha": "3.0.1",
    "mock-require": "1.3.0",
    "sinon": "2.3.6",
    "request": "latest",
    "nock": "latest",
    "chai-http": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:madeinstefano/relic_logs.git"
  },
  "keywords": [
    "logs",
    "request",
    "node"
  ],
  "engines": {
    "node": "8.1.3",
    "npm": "4.6.1"
  },
  "scripts": {
    "lint:lib": "eslint lib/ --fix",
    "lint:root": "eslint rlc.js --fix && eslint relic_logs.js --fix && eslint x_relic_logs.js --fix",
    "lint": "npm run lint:lib && npm run lint:root",
    "test:unit": "NODE_ENV=test mocha --env --report spec --recursive ./spec/unit --timeout 3000",
    "test:integration": "NODE_ENV=test mocha --env --report spec --recursive ./spec/integration --timeout 8000",
    "test": "npm run test:unit && npm run test:integration"
  },
  "contributors": [
    {
